{
  "name": "vite-web-component-library",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "main": "./dist/fvr-components.umd.js",
  "module": "./dist/fvr-components.es.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fvr-components.es.js",
      "require": "./dist/fvr-components.umd.js",
      "types": "./dist/types/index.d.ts"
    },
    "./react": {
      "import": "./dist/react.es.js",
      "types": "./dist/types/generated/react.d.ts"
    },
    "./jsx": {
      "types": "./dist/types/generated/jsx.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "build": "npm run generate:types && npm run build:main && npm run build:react",
    "build:main": "vite build",
    "build:react": "BUILD_TARGET=react vite build",
    "generate": "node generator.cjs",
    "generate:types": "node scripts/generate-types.cjs",
    "generate:watch": "node scripts/watch-types.cjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@chromatic-com/storybook": "1.6.1",
    "@storybook/addon-essentials": "8.2.8",
    "@storybook/addon-links": "8.2.8",
    "@storybook/blocks": "8.2.8",
    "@storybook/test": "8.2.8",
    "@storybook/web-components": "8.2.8",
    "@storybook/web-components-vite": "8.2.8",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "8.2.8",
    "typescript": "^5.3.0",
    "vite": "5.4.0",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  }
}
